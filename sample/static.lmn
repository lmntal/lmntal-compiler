{{
	{mv(m), m.incr, handle(X)}. io.print(param(value)).
	res(X), {shrink(X),value($n)} :- unary($n)|io.print($n).
}}.
{{{{
	{mv(m), m.incr, handle(X)}. res(X).
	res(X), {shrink(X),value($n)} :- unary($n)|io.print($n).
}}}}.
m : {
	name(m).
	static.load.
	id(0).
	{m.incr, handle(X), @r}, id($o)
		:- int($o),$n=$o+1
		| id($n), {shrink(X), value($n)}.
}.

